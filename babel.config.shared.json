{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": { "node": "8.3" },
        "modules": "commonjs"
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "allowNamespaces": true
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3,
        "useESModules": false,
        "version": "^7.8.4"
      }
    ]
  ]
}
